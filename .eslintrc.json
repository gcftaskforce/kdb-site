{
  "plugins": [
    "react",
    "jest"
  ],
  "env": {
    "jest/globals": true
  },
  "parserOptions": {
    "ecmaVersion": 2018,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "extends": "airbnb-base",
  "rules": {
    "strict": 0,
    "arrow-body-style": 0,
    "no-mixed-operators": 0,
    "max-len": 0,
    "no-unused-vars": [
      "error",
      { "varsIgnorePattern": "debug" }
    ]
  }
}
